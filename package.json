{
  "name": "front-end-guide",
  "version": "0.0.0",
  "description": "Manage your front-end project in unique views and reusable components.",
  "main": "source/index.js",
  "private": "yes",
  "scripts": {
    "build": "./node_modules/.bin/gulp build_guide && ./node_modules/.bin/gulp zip_dist",
    "build-assets": "./node_modules/.bin/gulp build_assets",
    "changelog": "node -e 'require(\"./lib/changelog\")()'",
    "release": "node ./lib/release.js",
    "release:patch": "node ./lib/release.js patch",
    "release:minor": "node ./lib/release.js minor",
    "release:major": "node ./lib/release.js major",
    "serve": "./node_modules/.bin/gulp serve",
    "start": "npm run serve",
    "test": "./node_modules/.bin/gulp test_run",
    "travis": "npm run test && npm run build && ./lib/travis-to-gh-pages.sh",
    "watch": "./node_modules/.bin/gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/voorhoede/front-end-guide.git"
  },
  "keywords": [
    "front-end",
    "guide",
    "patternlab",
    "reusable",
    "components",
    "modules"
  ],
  "author": "De Voorhoede",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voorhoede/front-end-guide/issues"
  },
  "homepage": "https://github.com/voorhoede/front-end-guide",
  "devDependencies": {
    "bower": "^1.3.12",
    "browser-sync": "^2.0.1",
    "chalk": "^0.5.1",
    "conventional-changelog": "0.0.11",
    "del": "^1.1.0",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-cached": "^1.0.1",
    "gulp-filter": "^2.0.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.1.0",
    "gulp-jscs": "^1.1.2",
    "gulp-jshint": "^1.9.2",
    "gulp-karma": "0.0.4",
    "gulp-less": "^3.0.0",
    "gulp-newer": "^0.5.0",
    "gulp-plumber": "^0.6.6",
    "gulp-recess": "^1.1.1",
    "gulp-rename": "^1.2.0",
    "gulp-replace": "^0.5.0",
    "gulp-sourcemaps": "^1.2.4",
    "gulp-util": "^3.0.1",
    "gulp-zip": "^2.0.2",
    "imagemin-pngquant": "^4.0.0",
    "inquirer": "^0.8.0",
    "jshint-stylish": "^1.0.0",
    "json-stable-stringify": "^1.0.0",
    "karma": "^0.12.24",
    "karma-chrome-launcher": "^0.1.5",
    "karma-jasmine": "^0",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-requirejs": "^0.2.2",
    "lodash-node": "^3.1.0",
    "minimist": "^1.1.0",
    "mkdirp": "^0.5.0",
    "nunjucks": "^1.1.0",
    "q": "^1.0.1",
    "requirejs": "^2.1.15",
    "rimraf": "^2.2.8",
    "run-sequence": "^1.0.1",
    "through2": "^1.1.1",
    "gulp-zip": "^2.0.2",
    "markdown-it": "^3.0.4"
  }
}
